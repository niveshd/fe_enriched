subsection geometry
    set dim = 2
    set size = 100
    set shape = 0
    set Global refinement = 7
    set cycles = 0
end

subsection solver
    set fe base degree = 1
    set fe enriched degree = 1
    set max iterations = 50000
    set tolerance = 1e-9
end

subsection expressions
    set sigma = 0.05

    set rhs expression =
    set boundary expression = 1.0/(2*pi*sigma*sigma)*exp(-(x*x + y*y)/(2*sigma*sigma))

    set rhs radial problem =
    set boundary radial problem = 1.0/(2*pi*sigma*sigma)*exp(-(x*x)/(2*sigma*sigma))

    set exact solution expression = 1.0/(2*pi*sigma*sigma)*exp(-(x*x + y*y)/(2*sigma*sigma))
    set estimate exact solution = false
end

subsection output
    set patches = 1
    set debug level = 1
end

