subsection geometry
    set dim = 2
    set size = 4
    set shape = 1
    set Global refinement = 3
    set cycles = 4
end

subsection solver
    set fe base degree = 1
    set fe enriched degree = 1
    set max iterations = 50000
    set tolerance = 1e-9
end

subsection expressions
    set rhs value = 1.0/(2*pi*sigma*sigma)*exp(-(x*x + y*y)/(2*sigma*sigma))
    set boundary value = 0

    set rhs value radial problem = 1.0/(2*pi*sigma*sigma)*exp(-(x*x)/(2*sigma*sigma))
    set boundary value radial problem = 0
    set sigma = 0.1
    set predicate radius = 0.4
end

subsection output
    set patches = 1
    set debug level = 1
end

